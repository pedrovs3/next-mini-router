{
  "singleQuote": true,
    "trailingComma": "all",
    "allowParens": "avoid",
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "useTabs": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "embeddedLanguageFormatting": "auto",
    "quoteProps": "as-needed",
    "insertPragma": false,
    "requirePragma": false
}
